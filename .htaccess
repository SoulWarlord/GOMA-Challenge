# Turn mod_rewrite on
#RewriteEngine On
#RewriteBase /GOMA/

# To externally redirect /projectdir/file.php to /projectdir/foo
#RewriteCond %{THE_REQUEST} \s/+GOMA/+(.*?/)?(?:index)?(.*?)\.php[\s?] [NC]
#RewriteRule ^ %1%2 [R=302,L,NE]

# To internally forward /projectdir/foo to /projectdir/foo.php
#RewriteCond %{REQUEST_FILENAME} !-d
#RewriteCond %{DOCUMENT_ROOT}/GOMA/$1\.php -f [NC]
#RewriteRule ^(.+?)/?$ $1.php [L]
